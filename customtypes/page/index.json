{
  "id": "page",
  "label": "Page",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "URL Slug",
          "placeholder": "about, contact, services, etc."
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Page Title",
          "placeholder": "Page title"
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "SEO title (defaults to Page Title if empty)"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "Page description for SEO"
        }
      },
      "body": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "hero_homepage": {
              "type": "SharedSlice"
            },
            "rich_text": {
              "type": "SharedSlice"
            },
            "two_column": {
              "type": "SharedSlice"
            },
            "features": {
              "type": "SharedSlice"
            },
            "stats": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "faq": {
              "type": "SharedSlice"
            },
            "cta": {
              "type": "SharedSlice"
            },
            "image_gallery": {
              "type": "SharedSlice"
            },
            "team_gallery": {
              "type": "SharedSlice"
            },
            "video_embed": {
              "type": "SharedSlice"
            },
            "logo_cloud": {
              "type": "SharedSlice"
            },
            "contact_form": {
              "type": "SharedSlice"
            },
            "newsletter_signup": {
              "type": "SharedSlice"
            },
            "spacer": {
              "type": "SharedSlice"
            },
            "divider": {
              "type": "SharedSlice"
            },
            "scroll_accordion": {
              "type": "SharedSlice"
            },
            "horizontal_gallery": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}